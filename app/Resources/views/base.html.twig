        {% block body %}{% endblock %}
      </div>
      <footer class="MainFooter">
        <div class="l__wrapper">
          <ul class="MainFooter-listLegalStuff">
            <li class="ListItem">
              <a href="#" class="MenuLink">Contacto</a>
            </li>
            <li class="ListItem">Sociedad Protectora de animales de la Ribera (Carcaixent - Valencia) Â© 2014</li>
          </ul>
          {% include '_social.html.twig' %}
        </div>
      </footer>
      <div class="overlay"></div>
    </div>
    {% block javascripts %}{% endblock %}
  </body>
</html>
